<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.minisite_project.admin.notice.mapper.NoticeMapper">


    <select id="selectListCount" resultType="long">
        select count(*)
        from notice
        where 1 = 1

    </select>

    <select id="selectList" resultType="com.example.minisite_project.admin.notice.dto.NoticeDto">
        select *
        from notice
        where 1 = 1

        order by reg_dt desc
        limit #{pageStart}, #{pageEnd}
    </select>

</mapper>
